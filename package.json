{
  "name": "threading-js",
  "version": "0.0.4",
  "description": "Small wrapper for web workers that allows for running tasks without having to serve a worker script to the client.",
  "main": "Thread.js",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.1",
    "static-server": "^2.0.6",
    "webpack": "^3.8.1"
  },
  "scripts": {
    "build-thread": "cross-env PACKAGE=Thread webpack",
    "build-threadpool": "cross-env PACKAGE=ThreadPool webpack",
    "build-threadqueue": "cross-env PACKAGE=ThreadQueue webpack",
    "build": "npm run build-thread & npm run build-threadpool & npm run build-threadqueue",
    "serve": "static-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gkjohnson/javascript-thread-runner.git"
  },
  "keywords": [
    "javascript",
    "performance",
    "threads",
    "webworker",
    "webworkers",
    "process",
    "subprocess"
  ],
